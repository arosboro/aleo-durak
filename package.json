    "@demox-labs/aleo-wallet-adapter-reactui": "^0.0.23",
    "@entropy1729/aleo-js": "^0.1.5",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "axios": "^1.2.2",
    "highlight.js": "^11.7.0",
    "pinia": "^2.0.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "veaury": "^2.3.11",
    "vite-plugin-wasm": "^3.1.1",
