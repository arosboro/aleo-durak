{
  "name": "durak",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build:aleo_bitwise_stack": "leo build --path deps/crypto/aleo_bitwise_stack",
    "build:aleo_casino_table": "leo build --path deps/crypto/aleo_casino_table",
    "slingshot": "npm run build:aleo_bitwise_stack; npm run build:aleo_casino_table; slingshot node start --key APrivateKey1zkp3sB1AJEkKkUtnz7Ye8TFFZYBkabN8h7NJr1toVrbZyLR",
    "pour": "npm run pour:aleo_bitwise_stack; npm run pour:aleo_casino_table",
    "pour:aleo_bitwise_stack": "slingshot pour aleo1g7lprd8dkrt76jnyrgnw4mw0cpe2u4ckkajs96wqu8xyggze6v8qlegxp3 1000000000",
    "pour:aleo_casino_table": "slingshot pour aleo1s2hkr9nw7vp2vrlfzewwluqqfg8pfykcffm9gx90qmhk8vrj7urqum9yqn 1000000000",
    "deploy": "npm run deploy:aleo_bitwise_stack; npm run deploy:aleo_casino_table",
    "deploy:aleo_bitwise_stack": "slingshot deploy -f 100000000 --path deps/crypto/aleo_bitwise_stack/build",
    "deploy:aleo_casino_table": "slingshot deploy -f 100000000 --path deps/crypto/aleo_casino_table/build"
  },
